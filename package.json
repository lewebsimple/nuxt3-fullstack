{
  "name": "@lewebsimple/nuxt3-fullstack",
  "description": "Exploring Nuxt3 as a full stack solution",
  "version": "0.7.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:codegen": "NODE_NO_WARNINGS=1 graphql-codegen",
    "build:nuxt": "nuxi build",
    "build": "run-s build:*",
    "dev:codegen": "NODE_NO_WARNINGS=1 graphql-codegen -w",
    "dev:nuxt": "nuxi dev",
    "dev": "run-p dev:*",
    "lint": "eslint --fix --ignore-path .gitignore .",
    "postinstall": "prisma generate",
    "release": "release-it",
    "start": "node .output/server/index.mjs"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "devDependencies": {
    "@formkit/nuxt": "^1.0.0-beta.6",
    "@formkit/tailwindcss": "^1.0.0-beta.6",
    "@formkit/vue": "^1.0.0-beta.6",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/near-operation-file-preset": "^2.2.8",
    "@graphql-codegen/typescript": "^2.4.7",
    "@graphql-codegen/typescript-operations": "^2.3.4",
    "@graphql-codegen/typescript-vue-urql": "^2.2.8",
    "@lewebsimple/eslint-config-vue": "^0.5.2",
    "@nuxtjs/tailwindcss": "^5.0.2",
    "@prisma/client": "^3.11.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.2",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@urql/devtools": "^2.0.3",
    "@urql/vue": "^0.6.1",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.11.0",
    "graphql": "^16.3.0",
    "graphql-helix": "^1.12.0",
    "jsonwebtoken": "^8.5.1",
    "nexus": "^1.3.0",
    "nexus-prisma": "^0.35.0",
    "nexus-shield": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "nuxt3": "latest",
    "prisma": "^3.11.0",
    "release-it": "^14.12.5",
    "ts-node": "^10.7.0"
  },
  "eslintConfig": {
    "extends": "@lewebsimple/eslint-config-vue"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    }
  }
}
