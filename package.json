{
  "name": "@lewebsimple/nuxt3-fullstack",
  "description": "Exploring Nuxt3 as a full stack solution",
  "version": "0.7.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "NODE_NO_WARNINGS=1 nuxi build",
    "dev": "NODE_NO_WARNINGS=1 nuxi dev",
    "lint": "eslint --fix --ignore-path .gitignore .",
    "postinstall": "prisma generate",
    "release": "release-it",
    "start": "node .output/server/index.mjs"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "devDependencies": {
    "@formkit/vue": "^1.0.0-beta.2",
    "@graphql-codegen/near-operation-file-preset": "^2.2.6",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-codegen/typescript-operations": "^2.3.2",
    "@graphql-codegen/typescript-vue-urql": "^2.2.6",
    "@lewebsimple/eslint-config-vue": "^0.5.2",
    "@nuxtjs/tailwindcss": "^5.0.0-4",
    "@prisma/client": "^3.10.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.2",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@urql/devtools": "^2.0.3",
    "@urql/vue": "^0.6.1",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.9.0",
    "graphql": "^16.3.0",
    "graphql-helix": "^1.11.0",
    "jsonwebtoken": "^8.5.1",
    "nexus": "^1.2.0",
    "nexus-prisma": "^0.35.0",
    "nexus-shield": "^2.1.0",
    "nuxt-graphql-codegen": "^0.1.2",
    "nuxt3": "latest",
    "prisma": "^3.10.0",
    "release-it": "^14.12.5",
    "ts-node": "^10.5.0"
  },
  "eslintConfig": {
    "extends": "@lewebsimple/eslint-config-vue"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    }
  }
}
